{
  "name": "vue-faq-accordion",
  "version": "1.3.0",
  "description": "",
  "main": "dist/vue-faq-accordion.umd.js",
  "module": "dist/vue-faq-accordion.esm.js",
  "unpkg": "dist/vue-faq-accordion.min.js",
  "browser": {
    "./sfc": "src/vue-faq-accordion.vue"
  },
  "scripts": {
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-faq-accordion.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-faq-accordion.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-faq-accordion.min.js"
  },
  "dependencies": {
    "vue-markdown": "^2.2.4",
    "vue2-transitions": "^0.2.3"
  },
  "devDependencies": {
    "minimist": "^1.2.0",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^3.0.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gerasimvol/vue-faq-accordion.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "accordion",
    "faq"
  ],
